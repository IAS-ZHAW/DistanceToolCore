<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:ditoConfiguration xmlns:ns2="http://ias.zhaw.ch/">
    <input>
        <filename>c:\asdf</filename>
        <separator>97</separator>
        <questionTitles>false</questionTitles>
        <allQuestions>true</allQuestions>
        <startQuestion>0</startQuestion>
        <endQuestion>10</endQuestion>
        <allSurveys>true</allSurveys>
        <startSurvey>0</startSurvey>
        <endSurvey>10</endSurvey>
    </input>
    <output>
        <filename>c:/bsdf</filename>
        <separator>98</separator>
        <precision>10</precision>
    </output>
    <method>
        <name>Canberra</name>
        <useRandomSample>false</useRandomSample>
        <sampleSize>500</sampleSize>
        <parallel>false</parallel>
        <numberOfThreads>20</numberOfThreads>
    </method>
    <questionConfig>
        <enableScale>false</enableScale>
        <enableAutoScale>false</enableAutoScale>
        <enableQuestionWeight>false</enableQuestionWeight>
        <enableDistanceWeight>true</enableDistanceWeight>
    </questionConfig>
    <questions>
        <question>
            <column>100</column>
            <name>name</name>
            <questionType>ORDINAL</questionType>
            <enabled>true</enabled>
            <scaling>100.0</scaling>
            <questionWeight>50.0</questionWeight>
            <distanceWeight>30.0</distanceWeight>
            <excludeValues>
                <value>3.31</value>
                <value>2.2</value>
                <value>1.0</value>
            </excludeValues>
            <offset>0.0</offset>
        </question>
    </questions>
</ns2:ditoConfiguration>
